<% include header.ejs %>

  <div class="container-fluid container">
      <h1><%= title %></h1>
          <div class="row">        
            <% for(var i=0; i<content.length; i++) {%>
               <div class="col-md-3 col-sm-6 col-xs-6" style="text-align:center;height:70px;">
                  <a href="/tables/summary/<%=content[i].id %>">
                    <button type="button" class="btn btn-default btn-lg <%= content[i].status == 1 ? 'active' : '' %>">
                      <span class="glyphicon   <%= content[i].status == 1 ? 'glyphicon-star-empty' : 'glyphicon-star' %>" aria-hidden="true"></span> <%=content[i].descricao %>
                    </button> 
                  </a>
               </div>
            <% } %>
          </div>
   </div>
   
<% include footer.ejs  %>
